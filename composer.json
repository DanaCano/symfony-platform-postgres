{
  "type": "project",
  "name": "platform/symfony-app",
  "description": "Plataforma (proyectos/postulaciones) en Symfony 6.4 + Twig + JS + Doctrine",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/asset": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dotenv": "^6.4",
    "symfony/error-handler": "^6.4",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-client": "^6.4",
    "symfony/runtime": "^6.4",
    "symfony/security-bundle": "^6.4",
    "symfony/string": "^6.4",
    "symfony/twig-bundle": "^6.4",
    "symfony/yaml": "^6.4",
    "doctrine/annotations": "^2.0",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/doctrine-migrations-bundle": "^3.5",
    "doctrine/orm": "^2.18"
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.60",
    "symfony/var-dumper": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.4.*"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  }
}